---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>http://elfjs.com/</loc>
		<lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
	</url>
	<url>
		<loc>http://elfjs.com/downloads/</loc>
		<lastmod>{{ site.release.latest.date }}</lastmod>
	</url>
	<url>
		<loc>http://elfjs.com/blog/</loc>
		<lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
	</url>
	{% for post in site.posts %}
	<url>
		<loc>http://elfjs.com{{ post.url }}</loc>
		<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
	</url>
	{% endfor %}
</urlset> 